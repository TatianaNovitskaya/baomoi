.header {

}

.header-top {
	height: rem(73)
	display: flex
	justify-content: space-between
	align-items: flex-end
	padding: 0.3rem
}
.header__logo{
	width: rem(70)
	height: rem(70);
	margin-left: rem(50)
	margin-right: rem(10)
}
.icon-svg {
	width: rem(32)
	height: rem(32)
	margin: 0 0.8rem 0 0
	cursor: pointer
}
.header-icon-2{

}
.header__icons {
	display: flex
}
.header_left{
	display: flex
	align-items: flex-end
}
.header__title {
	h2 {
		font-weight: bold;
		color: rgb(244, 81, 30);
		line-height: rem(34);
		text-shadow: 0px 0px 2px rgba(0, 0, 0, 1);
		font-size: rem(28)
	}
}

.header__list {
	background: #0092ea
	height: 2rem
	ul{
		display: flex
		align-items: center
		justify-content: space-between
	}
	//li{
	//
	//}
	a{
		text-decoration: none
		color: #fff;
		font-weight: 400;
		line-height: rem(21);
		font-size: rem(15)
	}
}